<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EmissionTracker">
        <attr name="timeLength" format="enum">
            <enum name="today" value="0" />
            <enum name="all_time" value="1" />
        </attr>
        <!-- Specifies how much emission has been contributed in number -->
        <attr name="emissionValue" format="integer" />
        <!-- Specifies maximum emission to calculate for percentage -->
        <attr name="emissionMaxValue" format="integer" />
    </declare-styleable>
    <declare-styleable name="TravelActivities">
        <attr name="walkDistance" format="integer" />
        <attr name="bikeDistance" format="integer" />
        <attr name="carDistance" format="integer" />
        <attr name="publicTransDistance" format="integer" />
    </declare-styleable>
</resources>